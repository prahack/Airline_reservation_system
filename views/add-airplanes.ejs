 <!-- start banner Area -->
<section class="banner-area relative">
    <div class="overlay overlay-bg">
    </div>				
    <div class="container">
        <div class="row fullscreen align-items-admin ">
            <form class="text-left form-wrap" action="" method="post" enctype="multipart/form-data">
                <% if (message != '') { %>
                    <p class="text-left text-danger" ><%= message %></p>
                <% } %>
                <div class="form-row">
                    <div class="form-group col-md-8">
                        <p style="color: rgb(5, 19, 145); font-size: 20px;">Fill this form to add an airplane.</p><br>
                    </div>
                    <div class="form-group col-md-8">
                        <p style="color: black; font-size: 15px;">Type of the airplane</p>
                        <select id="type" name="type" class="form-control" required>
                            <option selected disabled>Type</option>
                            <% aircrafts.forEach((aircraft, index) => { %>

                            <option><%= aircraft.type %></option>

                            <% }) %>

                        </select>
                    </div>
                    
                </div>
                <button type="submit" class="primary-btn" style="background-color: rgba(5, 5, 216, 0.829);">Add an airplane</button>	
            </form>
        </div>
        </div>
    </div>					
</section>
